{
    "name": "funayaki/assets",
    "description": "Assets plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4",
        "josegonzalez/cakephp-upload": "^3.8",
        "funayaki/utility": "^0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Assets\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
